<!-- 此处配置加载优先于 var.xml -->
<include>
	<!-- 外网IP配置 -->
	<X-PRE-PROCESS cmd="set" data="ext_ip=192.168.2.11" />

	<!-- internal SIP Profile SIP Port -->
	<X-PRE-PROCESS cmd="set" data="internal_sip_port=21010" />
	<X-PRE-PROCESS cmd="set" data="internal_tls_port=21011" />
	<X-PRE-PROCESS cmd="set" data="internal_ws_port=21016" />
	<X-PRE-PROCESS cmd="set" data="internal_wss_port=21017" />

	<!-- External SIP Profile SIP Port -->
	<X-PRE-PROCESS cmd="set" data="external_sip_port=21012" />
	<X-PRE-PROCESS cmd="set" data="external_tls_port=21013" />

	<!-- Media RTP port range -->
	<X-PRE-PROCESS cmd="set" data="rtp-start-port=21020" />
	<X-PRE-PROCESS cmd="set" data="rtp-end-port=21050" />

	<!-- Event Socket inBound -->
	<X-PRE-PROCESS cmd="set" data="esl_bind_ip=0.0.0.0" />
	<X-PRE-PROCESS cmd="set" data="esl_listen_port=21014" />
	<X-PRE-PROCESS cmd="set" data="esl_bind_pwd=long@123" />

	<!-- db 配置，mysql -->
	<X-PRE-PROCESS cmd="set" data="dbHost=192.168.2.62" />
	<X-PRE-PROCESS cmd="set" data="dbPort=6033" />
	<X-PRE-PROCESS cmd="set" data="dbName=aiswitch" />
	<X-PRE-PROCESS cmd="set" data="dbUser=xnjx" />
	<X-PRE-PROCESS cmd="set" data="dbPwd=xnjx@123!" />
	<X-PRE-PROCESS cmd="set" data="dbTimeout=10" />

	<!-- xml_rpc a simple webUi, X-PRE-PROCESS 不能直接注释，要修改前缀才有效-->
	<!-- <Z-PRE-PROCESS cmd="set" data="http-port=21015" />
	<Z-PRE-PROCESS cmd="set" data="auth-realm=aiswitch" />
	<Z-PRE-PROCESS cmd="set" data="auth-user=aiswitch" />
	<Z-PRE-PROCESS cmd="set" data="auth-pass=long@123" /> -->

	<!-- 默认全局日志级 Log Level - debug,info,notice,warning,err,crit,alert -->
	<X-PRE-PROCESS cmd="set" data="loglevel=debug" />
</include>